{
  "expo": {
    "name": "MediCare",
    "slug": "MediCare",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/medicare-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/medicare-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to upload medicine images.",
        "NSCameraUsageDescription": "This app needs access to camera to take photos of medicines for documentation."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/medicare-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.medicare.app",
      "versionCode": 3,
      "edgeToEdgeEnabled": true,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/medicare-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ca67fb89-f606-4023-84d4-0d994da5f0cd"
      }
    }
  }
}
